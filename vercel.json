{
  "version": 2,
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install --legacy-peer-deps",
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/images/(.*)", "dest": "/images/$1" },
    { "src": "/(.*)\\.js", "dest": "/$1.js" },
    { "src": "/(.*)\\.css", "dest": "/$1.css" },
    { "src": "/(.*)\\.svg", "dest": "/$1.svg" },
    { "src": "/(.*)\\.png", "dest": "/$1.png" },
    { "src": "/(.*)\\.jpg", "dest": "/$1.jpg" },
    { "src": "/(.*)\\.ico", "dest": "/$1.ico" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
